  "scripts": {
    "analyze:bundle:sourcemap": "source-map-explorer 'build/static/js/*.js'",
    "analyze:bundle:webpack": "http-server ./tmp/bundle-report -p 5000 -o",
    "build": "node scripts/build.js",
    "build:storybook": "build-storybook -s public",
    "format": "prettier --write src/**/*.{js,json,jsx,scss}",
    "format:check": "prettier --check src/**/*.{js,json,jsx}",
    "format:debug": "prettier --debug-check src/**/*.{js,json,jsx}",
    "lint:script": "eslint src --ext .js,.jsx",
    "lint:script:fix": "eslint --fix src --ext .js,.jsx",
    "lint:styles:css": "stylelint src/**/*.css",
    "lint:styles:scss": "stylelint src/**/*.scss",
    "lint:styles:styled": "stylelint src/**/*.{js,jsx} --config .stylelintrc.styled.json",
    "start": "node scripts/start.js",
    "start:https": "HTTPS=true yarn start",
    "start:static": "http-server ./build -p 8080 -o",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:static": "http-server ./storybook-static -p 8081 -o",
    "test": "node scripts/test.js --config jest.config.json",
    "test:coverage": "npm test -- --coverage --no-cache --silent",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:silent": "npm test -- --silent",
    "test:staged": "cross-env CI=true npm test",
    "test:verbose": "npm test -- --verbose"
  },
